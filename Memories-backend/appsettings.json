{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DevConnection": "Server=${DB_SERVER},${DB_PORT};Database=${DB_NAME};User Id=${DB_USER};Password=${DB_PASSWORD};TrustServerCertificate=True;"
  },
  "JWT": {
    "Issuer": "http://localhost:7207",
    "Audience": "http://localhost:3000",
    "Secret": "${JWT_SECRET}"
  },
  "Storage": {
    "Path": "C:\\ServerStorage"
  }
}
